{
  "name": "stormyy/b3cod4x",
  "description": "B3 management portal for Cod4x",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "stormyy/q3tool": "^0.2.1",
    "torann/geoip": "^1.0",
    "php": ">=7.0",
    "illuminate/support": "~5.4",
    "illuminate/console": "~5.4",
    "doctrine/dbal": "^2.5"
  },
  "autoload": {
    "psr-4": {
